                <string notr="true">/* Calendar Widget General */
QCalendarWidget {
    background-color: #ffffff;  /* Background color of the entire calendar widget */
    border: 1px solid #a1a1a1;  /* Border of the calendar widget */
    font-family: 'Arial', sans-serif;  /* Font family for the calendar text */
    font-size: 12px;  /* Font size for the calendar text */
    color: #333333;  /* Text color */
}

/* Navigation Bar */
QCalendarWidget QToolButton {
    height: 30px;  /* Height of the navigation buttons */
    width: 100px;  /* Width of the navigation buttons */
    color: #ffffff;  /* Text color of the navigation buttons */
    background-color: #4a90e2;  /* Background color of the navigation buttons */
    border: none;  /* Remove border of the navigation buttons */
    margin: 10px;  /* Margin around the navigation buttons */
}

QCalendarWidget QToolButton::hover {
    background-color: #357ab8;  /* Background color of the navigation buttons on hover */
}

QCalendarWidget QToolButton#qt_calendar_prevmonth {
    qproperty-icon: url(left_arrow.png);  /* Path to your left arrow icon */
}

QCalendarWidget QToolButton#qt_calendar_nextmonth {
    qproperty-icon: url(right_arrow.png);  /* Path to your right arrow icon */
}

/* Header Section (Month and Year) */
QCalendarWidget QSpinBox {
    width: 80px;  /* Width of the month/year selector */
    color: #ffffff;  /* Text color of the month/year selector */
    background-color: #4a90e2;  /* Background color of the month/year selector */
    border: none;  /* Remove border of the month/year selector */
}

QCalendarWidget QSpinBox::up-button {
    subcontrol-origin: border;  /* Origin of the up button control */
    subcontrol-position: top right;  /* Position of the up button control */
}

QCalendarWidget QSpinBox::down-button {
    subcontrol-origin: border;  /* Origin of the down button control */
    subcontrol-position: bottom right;  /* Position of the down button control */
}

QCalendarWidget QSpinBox::up-arrow {
    width: 10px;  /* Width of the up arrow */
    height: 10px;  /* Height of the up arrow */
}

QCalendarWidget QSpinBox::down-arrow {
    width: 10px;  /* Width of the down arrow */
    height: 10px;  /* Height of the down arrow */
}

/* Weekdays Section */
QCalendarWidget QTableView {
    alternate-background-color: #f2f2f2;  /* Alternate row background color */
}

QCalendarWidget QTableView QHeaderView::section {
    background-color: #4a90e2;  /* Background color of the weekdays header */
    color: #ffffff;  /* Text color of the weekdays header */
    padding: 5px;  /* Padding for the weekdays header */
    border: none;  /* Remove border of the weekdays header */
}

